<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_sub_list"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent"
    android:gravity="center"
    android:layoutDirection="ltr"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/sub_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="@dimen/px_16"
        android:background="@android:color/transparent"
        android:contentDescription="@string/desc"
        android:paddingLeft="@dimen/px_16"
        android:paddingRight="@dimen/px_16"
        android:paddingTop="@dimen/px_16"
        android:src="@drawable/ff" />

    <View
        android:id="@+id/line_close_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_0.5"
        android:background="@color/colorTextTitle" />

</LinearLayout>
